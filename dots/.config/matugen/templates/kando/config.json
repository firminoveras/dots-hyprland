{
  "locale": "auto",
  "showIntroductionDialog": false,
  "menuTheme": "default",
  "darkMenuTheme": "default",
  "menuThemeColors": {
    "default": {
      "hover-color": "{{colors.primary_container.default.hex}}",
      "background-color": "{{colors.surface.default.hex}}",
      "text-color": "{{colors.on_surface.default.hex}}",
      "border-color": "{{colors.outline.default.hex}}"
    }
  },
  "darkMenuThemeColors": {
    "default": {
      "hover-color": "{{colors.primary_container.default.hex}}",
      "background-color": "{{colors.surface.default.hex}}",
      "text-color": "{{colors.on_surface.default.hex}}",
      "border-color": "{{colors.outline.default.hex}}"
    }
  },
  "enableDarkModeForMenuThemes": false,
  "soundTheme": "simple-clicks",
  "soundVolume": 0.5,
  "ignoreWriteProtectedConfigFiles": false,
  "settingsWindowColorScheme": "dark",
  "settingsWindowFlavor": "sakura-dark",
  "trayIconFlavor": "white",
  "lazyInitialization": false,
  "enableVersionCheck": false,
  "zoomFactor": 0.7,
  "centerDeadZone": 50,
  "minParentDistance": 150,
  "dragThreshold": 15,
  "fadeInDuration": 200,
  "fadeOutDuration": 100,
  "keepInputFocus": false,
  "enableMarkingMode": true,
  "enableTurboMode": true,
  "warpMouse": true,
  "hoverModeNeedsConfirmation": false,
  "gestureMinStrokeLength": 150,
  "gestureMinStrokeAngle": 20,
  "gestureJitterThreshold": 10,
  "gesturePauseTimeout": 100,
  "fixedStrokeLength": 0,
  "rmbSelectsParent": false,
  "enableGamepad": true,
  "gamepadBackButton": 1,
  "gamepadCloseButton": 2,
  "sameShortcutBehavior": "close",
  "useDefaultOsShowSettingsHotkey": true,
  "hideSettingsButton": true,
  "settingsButtonPosition": "bottom-left",
  "editorOptions": {
    "showSidebarButtonVisible": true,
    "showEditorButtonVisible": true
  }
}
